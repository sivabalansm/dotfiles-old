URxvt.font: xft:monospace:size=13
URxvt.scrollBar: false
URxvt.saveLines: 65535
!fake transparency 
!URxvt.transparent: true
!URxvt.shading: 10


!Keybindings for up and down in the terminal (not needed with tmux)
!URxvt.keysym.C-k: command:\033]720;1\007
!URxvt.keysym.C-j: command:\033]721;1\007
!URxvt.keysym.C-u: eval:scroll_up_pages 1
!URxvt.keysym.Control-d: eval:scroll_down_pages 1

!perl script keybindings
!Copy in terminal to clipboard w/ vim mode
URxvt.perl-ext-common: keyboard-select,eval
URxvt.keyboard-select.clipboard: true
URxvt.keysym.C-n: perl:keyboard-select:activate

!Ctrl-shit-v and c for pasting and copying
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
!Disable the old control-alt-v and c for copying and pasting
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

!Disable these modes that activate on Ctrl-Shift
URxvt.iso14755: false
URxvt.iso14755_52: false

!Some extra mix keybinds
!Clear the window with Ctrl-l
urxvt.keysym.Control-l: clear\n
!Open ranger
urxvt.keysym.Control-f: ranger\n
URxvt.dynamicColors: on

